<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config></context:annotation-config>

    <bean id = "humanHeart" class="com.bl.autowire.Heart">
        <property name="nameOfAnimal" value="Human"></property>
        <property name="numOfHeart" value="1"></property>
    </bean>

    <bean id = "octopusHeart" class="com.bl.autowire.Heart">
        <property name="nameOfAnimal" value="Octopus"></property>
        <property name="numOfHeart" value="3"></property>
    </bean>

    <bean id="human" class="com.bl.autowire.Human"></bean>
</beans>